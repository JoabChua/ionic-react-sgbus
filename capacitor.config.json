{
  "appId": "io.devjo.transport",
  "appName": "transport-guardian",
  "webDir": "build",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": false,
      "androidSplashResourceName": "splash",
      "showSpinner": true,
      "androidSpinnerStyle": "small",
